{
  "name": "metabob-vscode",
  "displayName": "Metabob VScode",
  "description": "Metabob Vscode Extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Linters",
    "Programming Languages",
    "Snippets",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "linter",
    "lint",
    "code",
    "google",
    "api",
    "aip",
    "metabob"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "metabob-webview",
          "title": "Metabob Extension",
          "icon": "./src/assets/extension-icon.png"
        }
      ]
    },
    "views": {
      "metabob-webview": [
        {
          "type": "webview",
          "id": "suggestion-panel-webview",
          "name": "Suggestion",
          "icon": "./src/assets/extension-icon.png"
        },
        {
          "type": "webview",
          "id": "recommendation-panel-webview",
          "name": "Recommendation",
          "icon": "./src/assets/extension-icon.png"
        }
      ]
    },
    "commands": [
      {
        "command": "webview.open",
        "title": "Open Webview",
        "category": "Metabob"
      },
      {
        "command": "extension.sendMessage",
        "title": "Send Message to Webview",
        "category": "Metabob"
      }
    ],
    "configuration": {
      "title": "Metabob Extension configuration",
      "properties": {
        "metabob.apiKey": {
          "description": "API key to use for authentication",
          "type": "string",
          "default": "",
          "category": "Metabob"
        },
        "metabob.apiBaseUrl": {
          "description": "Endpoint of the backend service",
          "type": "string",
          "default": "",
          "category": "Metabob"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "axios": "^1.3.4",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rusty-result-ts": "^0.1.0"
  }
}
